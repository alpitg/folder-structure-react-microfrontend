{
  "version": "1.0",
  "name": "Micro Posterica",
  "description": "A microservice for managing posterica data",
  "author": "Posterica Team",
  "logo": "/static/media/img/logo.png",
  "api": {
    "baseUrl": "http://localhost:8000",
    "apiVersion": "v1",
    "profile": {
      "posterica": "/api/profile/frame-types"
    },
    "master": {
      "frameTypes": "/api/master/frame-types",
      "glassTypes": "/api/master/glass-types",
      "miscCharges": "/api/master/misc-charges"
    },
    "order": {
      "list": "/api/orders/search",
      "detail": "/api/orders/{orderId}",
      "placeOrder": "/api/orders/place-order",
      "updateOrder": "/api/orders/{orderId}"
    },
    "customer": {
      "list": "/api/customer",
      "create": "/api/customers/create",
      "update": "/api/customers/update",
      "delete": "/api/customers/delete"
    }
  }
}
